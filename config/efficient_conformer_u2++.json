{
    "model": {
        "encoder": {
            "output_size": 256,
            "attention_heads": 8,
            "linear_units": 2048,
            "num_blocks": 12,
            "dropout_rate": 0.1,
            "positional_dropout_rate": 0.1,
            "attention_dropout_rate": 0.1,
            "input_layer": "conv2d2",
            "normalize_before": true,
            "cnn_module_kernel": 15,
            "use_cnn_module": true,
            "activation_type": "swish",
            "pos_enc_layer_type": "rel_pos",
            "selfattention_layer_type": "rel_selfattn",
            "causal": true,
            "use_dynamic_chunk": true,
            "cnn_module_norm": "layer_norm",
            "use_dynamic_left_chunk": false,
            "efficient_conf": {
                "stride_layer_idx": [
                    3,
                    7
                ],
                "stride": [
                    2,
                    2
                ],
                "group_layer_idx": [
                    3,
                    7
                ],
                "group_size": 3,
                "stride_kernel": false
            }
        },
        "decoder": {
            "attention_heads": 8,
            "linear_units": 2048,
            "num_blocks": 3,
            "r_num_blocks": 3,
            "dropout_rate": 0.1,
            "positional_dropout_rate": 0.1,
            "self_attention_dropout_rate": 0.1,
            "src_attention_dropout_rate": 0.1
        },
        "ctc_weight": 0.3,
        "lsm_weight": 0.1,
        "length_normalized_loss": false,
        "reverse_weight": 0.3,
        "ctc_reduction": "mean",
        "ctc_zero_inf": false,
        "vocab_size": 72
    },
    "data": {
        "audio": {
            "window_stride_sec": 0.01,
            "window_size_sec": 0.025,
            "sample_rate": 16000,
            "window": "hamming",
            "normalize": false,
            "spec_augment": true,
            "n_mels": 80,
            "time_mask_para": 40,
            "freq_mask_para": 20,
            "time_mask_cnt": 1,
            "freq_mask_cnt": 1,
            "pad_token_id": 0
        },
        "text": {
            "pad_token_id": 0,
            "bos_token_id": 2,
            "eos_token_id": 3
        }
    }
